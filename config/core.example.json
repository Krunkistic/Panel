{
  "web": {
    "host": "0.0.0.0",
    "listen": 8080,
    "ssl": {
      "enabled": false,
      "certificate": "",
      "key": ""
    }
  },
  "mysql": {
    "host": "0.0.0.0",
    "user": "controlpanel",
    "password": "controlpanel",
    "database": "cp"
  },
  "sftp": {
    "path": "/srv/daemon-data",
    "ip": "0.0.0.0",
    "port": 2022,
    "keypair": {
      "bits": 2048,
      "e": 65537
    }
  },
  "docker": {
    "network": {
      "name": "controlpanel_nw"
    },
    "socket": "/var/run/docker.sock",
    "autoupdate_images": true,
    "timezone_path": "/etc/timezone",
    "interface": "172.18.0.1"
  },
  "filesystem": {
    "server_logs": "/tmp/controlpanel"
  },
  "remote": {
    "base": "https://panel.controlpanel.gg"
  },
  "uploads": {
    "size_limit": 1024
  }
}
